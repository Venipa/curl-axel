{
    "name": "venipa/curl-axel",
    "description": "PHP based download accelerator",
    "license": "MIT",
    "keywords": ["php", "curl", "curl-multi", "download", "accelerator", "composer", "http"],
    "authors": [
        {
            "name": "Jacer Omri",
            "email": "jaceromri@gmail.com"
        },
        {
            "name": "Venipa",
            "email": "admin@venipa.net",
            "homepage": "https://venipa.net",
            "role": "contributor"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "php-curl-class/php-curl-class": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "CurlAxel\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "scripts": {
        "test": "",
        "test-ci": ""
    },
    "extra": {
        "branch-alias": {
            "develop": "1.0-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "3.*"
    }
}
